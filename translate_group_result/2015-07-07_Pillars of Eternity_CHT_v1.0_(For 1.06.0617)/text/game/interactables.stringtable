			<FemaleText/>
		</Entry>
		<Entry>
			<ID>329</ID>
			<DefaultText>艾德邁斯的巢穴</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>330</ID>
			<DefaultText>技師大廳加成</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>331</ID>
			<DefaultText>植物花園加成</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>332</ID>
			<DefaultText>禮拜堂加成</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>333</ID>
			<DefaultText>鍛冶店加成</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>334</ID>
			<DefaultText>會議所加成</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>335</ID>
