    <Entry>
      <ID>6</ID>
      <DefaultText>“你是谁？”</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>[保持沉默。]</DefaultText>
      <FemaleText />
