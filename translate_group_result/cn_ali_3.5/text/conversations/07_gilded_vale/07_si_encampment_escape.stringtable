			<FemaleText/>
		</Entry>
		<Entry>
			<ID>11</ID>
			<DefaultText>把武器朝攻击者扔去。</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>12</ID>
			<DefaultText>允许希奥丹自由活动。</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>14</ID>
			<DefaultText>向攻击者开火。</DefaultText>
			<FemaleText/>
		</Entry>
