			<DefaultText>[派遣[Slot 4]。]</DefaultText>
			<FemaleText/>
		</Entry>
		<Entry>
			<ID>13</ID>
			<DefaultText>[Send [Slot 5].]</DefaultText>
			<FemaleText/>
		</Entry>
