<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\prototype_2\si_party_check_proto</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>6</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>A broken bridge lies before you. It looks as if you can jump across the gap. What do you do?</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>[Jump the chasm.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>[Leave.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>{party athletics &gt; 10} The party clears the gap without incident.</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>{one member athletics &lt; 10 and is lowest in party} While the party clears the gap, {name token} turns an ankle in the attempt. {give_status_hobbled}</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>{multiple member athletics &lt; 10} The party clears the gap, but the exertion of the feat leaves some party members winded. {give_status_weakend}</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
</StringTableFile>