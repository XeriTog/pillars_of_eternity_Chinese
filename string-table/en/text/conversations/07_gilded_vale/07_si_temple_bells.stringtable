      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>[Silence the bell.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>8</ID>
      <DefaultText>[Ring the left, smaller bell.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>9</ID>
      <DefaultText>[Ring the right, smaller bell.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>11</ID>
