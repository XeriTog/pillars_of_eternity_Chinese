    <Entry>
      <ID>16</ID>
      <DefaultText>[Leave the rubble alone.]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>17</ID>
      <DefaultText>You step back and consider your options.</DefaultText>
