<?xml version="1.0" encoding="utf-8"?>
<StringTableFile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Name>conversations\06_stronghold\06_bs_gramrfel_prisoner</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>6</EntryCount>
  <Entries>
    <Entry>
      <ID>1</ID>
      <DefaultText>"Hrm. A fine place to find myself." </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>2</ID>
      <DefaultText>"I don't suppose I could convince you to release me out of the kindness of your own heart?" </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"If you cut down all your captives, you'll find yourself rather alone down here." </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"You... have a monstrous appetite for inflicting pain. I recognize the affliction, you see." </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Will you leave me to rot in here? Let me free! You'll profit by it." </DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"You... find me at a bad time... my lord." </DefaultText>
      <FemaleText>"You... find me at a bad time... my lady." </FemaleText>
    </Entry>
  </Entries>
</StringTableFile>